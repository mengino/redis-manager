{
    "name": "mengino/redis-manager",
    "description": "Redis manager for laravel",
    "type": "library",
    "keywords": ["laravel", "redis", "manager"],
    "homepage": "https://github.com/mengino/redis-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "mengino",
            "email": "mengino@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/contracts": ">=5.7",
        "illuminate/support": ">=5.7",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "laravel/laravel": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "Encore\\RedisManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\RedisManager\\RedisManagerServiceProvider"
            ]

        }
    }
}
